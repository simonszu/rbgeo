<%= header_partial %>
<h1>Cacheliste</h1>
<table class="table table-striped">
<% caches.each do |cache| %>
<tr>
  <td>
    <% if cache[14].eql? "Found it" %>
      <img src="http://www.geocaching.com/images/logtypes/2.png">
    <% elsif cache[14].eql? "Will Attend" %>
      <img src="http://www.geocaching.com/images/logtypes/9.png">
    <% elsif cache[14].eql? "Owner Maintenance" %>
      <img src="http://www.geocaching.com/images/logtypes/46.png">
    <% elsif cache[14].eql? "Needs Archived" %>
      <img src="http://www.geocaching.com/images/logtypes/7.png">
    <% elsif cache[14].eql? "Didn't find it" %>
      <img src="http://www.geocaching.com/images/logtypes/3.png">
    <% elsif cache[14].eql? "Archive" %>
      <img src="http://www.geocaching.com/images/logtypes/5.png">
    <% elsif cache[14].eql? "Write note" %>
      <img src="http://www.geocaching.com/images/logtypes/4.png">
    <% elsif cache[14].eql? "Temporarily Disable Listing" %>
      <img src="http://www.geocaching.com/images/logtypes/22.png">
    <% elsif cache[14].eql? "Attended" %>
      <img src="http://www.geocaching.com/images/logtypes/10.png">
    <% elsif cache[14].eql? "Webcam Photo Taken" %>
      <img src="http://www.geocaching.com/images/logtypes/11.png">
    <% elsif cache[14].eql? "Post Reviewer Note" %>
      <img src="http://www.geocaching.com/images/logtypes/18.png">
    <% elsif cache[14].eql? "Enable Listing" %>
      <img src="http://www.geocaching.com/images/logtypes/23.png">
    <% elsif cache[14].eql? "Needs Maintenance" %>
      <img src="http://www.geocaching.com/images/logtypes/45.png">
    <% end %>
  </td>
  <td>
    <% if cache[16] == 1 %>
      <img src="http://www.geocaching.com/images/icons/icon_fav.png">
    <% end %>
  </td>
  <td>
    <% if cache[4].eql? "Traditional Cache" %>
      <img src="http://www.geocaching.com/images/wpttypes/sm/2.gif">
    <% elsif cache[4].eql? "Multi-cache" %>
      <img src="http://www.geocaching.com/images/wpttypes/sm/3.gif"> 
    <% elsif cache[4].eql? "Unknown Cache" %>
      <img src="http://www.geocaching.com/images/wpttypes/sm/8.gif"> 
    <% elsif cache[4].eql? "Earthcache" %>
      <img src="http://www.geocaching.com/images/wpttypes/sm/earthcache.gif">
    <% elsif cache[4].eql? "Event Cache" %>
      <img src="http://www.geocaching.com/images/wpttypes/sm/6.gif">
    <% elsif cache[4].eql? "Webcam Cache" %>
      <img src="http://www.geocaching.com/images/wpttypes/sm/11.gif">
    <% elsif cache[4].eql? "Lost and Found Event Cache" %>
      <img src="http://www.geocaching.com/images/wpttypes/sm/3653.gif">
    <% end %>
    <a href = "details/<%= cache[1] %>.html">
      <% if (cache[11].eql? "Disabled") || (cache[11].eql? "Archived") %>
        <del>
      <% end %>
      <%= cache[2] %> 
      <% if (cache[11].eql? "Disabled") || (cache[11].eql? "Archived") %>
        </del>
      <% end %>
      </a>
    <br />
  </td>
</tr>
<% end %>
<%= footer_partial %>