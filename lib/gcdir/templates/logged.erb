<%= header_partial %>
<h1>Von <%= name %> geloggte Caches</h1>
<table class="table table-striped">
<tr>
<th>#</th>
<th>Logtyp</th>
<th>Name</th>
<th>Typ</th>
<th>GC-ID</th>
<th>Favorit</th>
<th>Datum</th>
</tr>
<% Cache.all.reverse.each do |cache| %>
<tr>
  <td><%= cache.id %></td>
    <td>
    <% if cache.logtype.eql? "Found it" %>
      <img src="http://www.geocaching.com/images/logtypes/2.png">
    <% elsif cache.logtype.eql? "Will Attend" %>
      <img src="http://www.geocaching.com/images/logtypes/9.png">
    <% elsif cache.logtype.eql? "Owner Maintenance" %>
      <img src="http://www.geocaching.com/images/logtypes/46.png">
    <% elsif cache.logtype.eql? "Needs Archived" %>
      <img src="http://www.geocaching.com/images/logtypes/7.png">
    <% elsif cache.logtype.eql? "Didn't find it" %>
      <img src="http://www.geocaching.com/images/logtypes/3.png">
    <% elsif cache.logtype.eql? "Archive" %>
      <img src="http://www.geocaching.com/images/logtypes/5.png">
    <% elsif cache.logtype.eql? "Write note" %>
      <img src="http://www.geocaching.com/images/logtypes/4.png">
    <% elsif cache.logtype.eql? "Temporarily Disable Listing" %>
      <img src="http://www.geocaching.com/images/logtypes/22.png">
    <% elsif cache.logtype.eql? "Attended" %>
      <img src="http://www.geocaching.com/images/logtypes/10.png">
    <% elsif cache.logtype.eql? "Webcam Photo Taken" %>
      <img src="http://www.geocaching.com/images/logtypes/11.png">
    <% elsif cache.logtype.eql? "Post Reviewer Note" %>
      <img src="http://www.geocaching.com/images/logtypes/18.png">
    <% elsif cache.logtype.eql? "Enable Listing" %>
      <img src="http://www.geocaching.com/images/logtypes/23.png">
    <% elsif cache.logtype.eql? "Needs Maintenance" %>
      <img src="http://www.geocaching.com/images/logtypes/45.png">
    <% end %>
  </td>
  <td>
    <a href = "details/<%= cache.gcid %>.html">
      <% if (cache.status.eql? "Disabled") || (cache.status.eql? "Archived") %>
        <del>
      <% end %>
      <%= cache.name %>
      <% if (cache.status.eql? "Disabled") || (cache.status.eql? "Archived") %>
        </del>
      <% end %>
    </a>
  </td>
    <td>
    <% if cache.cachetype.eql? "Traditional Cache" %>
      <img src="http://www.geocaching.com/images/wpttypes/sm/2.gif">
    <% elsif cache.cachetype.eql? "Multi-cache" %>
      <img src="http://www.geocaching.com/images/wpttypes/sm/3.gif">
    <% elsif cache.cachetype.eql? "Unknown Cache" %>
      <img src="http://www.geocaching.com/images/wpttypes/sm/8.gif">
    <% elsif cache.cachetype.eql? "Earthcache" %>
      <img src="http://www.geocaching.com/images/wpttypes/sm/earthcache.gif">
    <% elsif cache.cachetype.eql? "Event Cache" %>
      <img src="http://www.geocaching.com/images/wpttypes/sm/6.gif">
    <% elsif cache.cachetype.eql? "Webcam Cache" %>
      <img src="http://www.geocaching.com/images/wpttypes/sm/11.gif">
    <% elsif cache.cachetype.eql? "Wherigo Cache" %>
      <img src="http://www.geocaching.com/images/wpttypes/sm/1858.gif">
    <% elsif cache.cachetype.eql? "Giga-Event Cache" %>
      <img src="http://www.geocaching.com/images/wpttypes/sm/7005.gif">
    <% end %>
  </td>
  <td>
    <a href="http://coord.info/<%= cache.gcid %>">
      <%= cache.gcid %>
    </a>
  <td>
    <% if cache.favorite == 1 %>
      <img src="http://www.geocaching.com/images/icons/icon_fav.png">
    <% end %>
  </td>
  <td>
    <%= Time.at(cache.logdate).strftime('%d.%m.%Y') %>
  </td>
</tr>
<% end %>
<%= footer_partial %>
